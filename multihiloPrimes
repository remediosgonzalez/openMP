        rango=int(num/hilos)
        inicio=0
        print(rango)
        for num_hilo in range(hilos):
            fin = inicio+rango
            if fin > num:
                fin = num
            hilo = threading.Thread(args=(inicio, fin, array_primos,num_hilo),target=set_primos)    
            hilo.start()
            hilo.join()
            inicio = fin
    return array_primos


primos=rango_primos(num)
